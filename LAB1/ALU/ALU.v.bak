module ALU #(parameter W=5)
		(input [W-1:0] A,
		input [W-1:0] B,
		input [2:0] ALUcntrl,
		output [W-1:0] O,
		output CO,
		output OVF,
		output N,
		output Z,
		);

case(ALUcntrl)
	3'b000:
		begin
		{CO,O} = A+B;
		Z = (O==0) ? 1:0;
		OVF=0;
		N=O[W-1];
	3'b001:
		begin
		{CO,O} = A-B;
		Z = (O==0) ? 1:0;
		OVF=0;
		N=O[W-1];	
endcase
endmodule 